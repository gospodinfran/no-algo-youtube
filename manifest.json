{
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "name": "No-algo YouTube",
  "description": "Turn YouTube into a search-only website.",
  "version": "0.0.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "yt-thumbnail128.png"
  },
  "permissions": ["activeTab", "scripting", "storage", "tabs"]
}
